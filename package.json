{
  "name": "blix",
  "version": "1.1.0",
  "description": "Yeoman on steroids: quickly create a multitude of Javascript applications from scratch and automate tasks with hackable scrips or add scripts and tools to existing projects.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "prettier --single-quote --no-semi --write '**/*.js'",
    "test": "jest --watch",
    "test:coverage": "jest --collectCoverage",
    "test:ci": "jest --coverage",
    "send-coverage": "./node_modules/.bin/codecov"
  },
  "keywords": [
    "express",
    "postgres",
    "bookshelf",
    "knex",
    "react",
    "redux",
    "react-router",
    "mongoose",
    "mongodb"
  ],
  "author": "Devon Bull",
  "license": "MIT",
  "bin": {
    "blix": "./index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "glob": "^7.1.2",
    "helmet": "^3.14.0",
    "inquirer": "^4.0.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.5"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "sinon": "^7.0.0"
  },
  "homepage": "https://github.com/blixjs/blix"
}
